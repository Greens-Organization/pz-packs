{
  "$schema": "https://turborepo.org/schema.json",
  "tasks": {
    "dev": {
      "cache": false,
      "persistent": true
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".vite/dist/**", "api.exe"]
    },
    "preview": {
      "dependsOn": ["build"],
      "outputs": []
    },
    "lint": {
      "outputs": []
    },
    "type:check": {
      "outputs": []
    },
    "docker:up": {
      "outputs": [],
      "cache": false
    },
    "docker:down": {
      "outputs": [],
      "cache": false
    },
    "db:studio": {
      "outputs": [],
      "cache": false
    }
  }
}
